    <link rel="shortcut icon" type="image/x-icon" href="images/ev-logo-02.png">
  
    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="css/bootstrap.min.css">
  
    <!--CSS Plugins-->
    <link rel="stylesheet" href="css/plugin.css">
    <!-- Default CSS-->
    <link rel="stylesheet" href="css/default.css">
    <!--Custom CSS-->
    <link rel="stylesheet" href="css/styles.css">
  
    <!--FontAwesome CSS-->
    <link rel="stylesheet" href="icons/font-awesome.min.css">
    <link href="https://fonts.googleapis.com/css2?family=Poppins:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap" rel="stylesheet">
  
  </head>
<body>
  <!-- Header Section Starts -->
  <%- include('../partials/Header') %>
  <section class="banner breadcrumb mb-0 pt-11 pb-10 position-relative rounded3 rounded-top-0">
    <div class="overlay rounded3 rounded-top-0"></div>
    <div class="container position-relative">
      <div class="banner-inner pt-10 w-lg-75 m-auto text-center text-white ">
        <div class="section-title mb-2">
          <h1 class="text-white mb-2">Our Services</h1>
          <ul class="list-unstyled d-flex justify-content-center">
            <li><a href="/">Home</a></li>
            <li class="mx-4"><i class="fa fa-star-o green" aria-hidden="true"></i></li>
            <li><a href="">Services</a></li>
          </ul>
        </div>
      </div>
    </div>
  </section>

  <section class="service pb-10">
    <div class="container">
      <div class="service-inner">
        <div class="section-title mb-6 text-center text-lg-start pt-6  ">
            <h2 class="ps-4 border-start border-3 border-green">Book your ride in seconds.  <span class="green">Affordable, safe, and available 24/7 for city rides, airport transfers, and outstation travel.</span> </h2>

     
        <div class="section-body">
          <div class="row  gy-4">
            <div class="col-lg-4 col-md-6">
              <div class="service-box p-6 border bg-white border-2 border-lightgreen rounded2 shadow">
                <div class="service-icon mb-4">
                  <img src="images/PNG/one-way-trip.png" class="w-20 h-auto" alt="service-icon">
                </div>
                <div class="service-info">
                  <h4><a href="service-detail.html" class="black">ONE-WAY TAXI</a></h4>
                  <p class="mt-2 mb-4">Why pay for the round trip when you only need a drop? Our one-way taxi service offers unbeatable rates for single-direction travel—perfect for smart, budget-conscious riders with simple needs.</p>
                  <a href="service-detail.html" class="btn border border-1 border-green">Book Now</a>
                </div>
              </div>
            </div>
            <div class="col-lg-4 col-md-6">
              <div class="service-box p-6 border border-2 bg-white border-lightgreen rounded2 shadow">
                <div class="service-icon mb-4">
                  <img src="images/PNG/rount-trip.png" class="w-20 h-auto" alt="service-icon">
                </div>
                <div class="service-info">
                  <h4><a href="service-detail.html" class="black">ROUND TRIP TAXI</a></h4>
                  <p class="mt-2 mb-4">Double the journey, double the comfort! Round trip taxis make travel easier with to-and-fro convenience—ideal for family holidays, office meetings, leisure rides, or weekend escapes without hassle or stress.</p>
                  <a href="service-detail.html" class="btn border border-1 border-green">Book Now</a>
                </div>
              </div>
            </div>
            <div class="col-lg-4 col-md-6">
              <div class="service-box p-6 border border-2 bg-white border-lightgreen rounded2 shadow">
                <div class="service-icon mb-4">
                  <img src="images/PNG/airport-pickup.png" class="w-20 h-auto" alt="service-icon">
                </div>
                <div class="service-info">
                  <h4><a href="service-detail.html" class="black">AIRPORT PICKUP TAXI</a></h4>
                  <p class="mt-2 mb-4">Never miss a flight again! Our airport taxi service provides reliable, on-time pickups with 24/7 availability and smooth, stress-free rides to or from the terminal—because your journey starts with us.

                  </p>
                  <a href="service-detail.html" class="btn border border-1 border-green">Book Now</a>
                </div>
              </div>
            </div>
            <div class="col-lg-4 col-md-6">
              <div class="service-box p-6 border border-2 bg-white border-lightgreen rounded2 shadow">
                <div class="service-icon mb-4">
                  <img src="images/PNG/airport-drop.png" class="w-20 h-auto" alt="service-icon">
                </div>
                <div class="service-info">
                  <h4><a href="service-detail.html" class="black">AIRPORT DROP TAXI</a></h4>
                  <p class="mt-2 mb-4">Just need a drop? We’ve got you covered. Our drop taxi service is fast, efficient, and affordable—no hidden charges, no detours—just a smooth ride to your exact destination, on time.</p>
                  <a href="service-detail.html" class="btn border border-1 border-green">Book Now</a>
                </div>
              </div>
            </div>
          
          </div>
        </div>
      </div>
    </div>
  </section>
<%- include('../partials/Footer') %>

  <!-- Footer Section Starts -->

  <!--Back-to-top Button start-->
  <div id="back-to-top">
    <a href="#" class="bg-green position-relative align-items-center rounded-circle d-block border border-4 border-grey"></a>
  </div>
